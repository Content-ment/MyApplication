<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">


    <android.support.constraint.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="411dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="330dp"
        android:layout_height="176dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_ZERO2"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:text="0"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/button_RESET2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.36666666667" />

    <Button
        android:id="@+id/button_RESET2"
        android:layout_width="219dp"
        android:layout_height="74dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:text="C"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_ZERO2"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.63333333333" />

    <Button
        android:id="@+id/button_ONE2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:text="1"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/button_TWO2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_ZERO2" />

    <Button
        android:id="@+id/button_TWO2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:text="2"
        android:textSize="32sp"
        app:layout_constraintEnd_toStartOf="@+id/button_THREE2"
        app:layout_constraintStart_toEndOf="@+id/button_ONE2"
        app:layout_constraintTop_toBottomOf="@+id/button_RESET2" />

    <Button
        android:id="@+id/button_THREE2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:text="3"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_TWO2"
        app:layout_constraintTop_toBottomOf="@+id/button_RESET2" />

    <Button
        android:id="@+id/button_FOUR2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:text="4"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/button_FIVE2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_ONE2" />

    <Button
        android:id="@+id/button_FIVE2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:text="5"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/button_SIX2"
        app:layout_constraintStart_toEndOf="@+id/button_FOUR2"
        app:layout_constraintTop_toBottomOf="@+id/button_TWO2" />

    <Button
        android:id="@+id/button_SIX2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:text="6"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_FIVE2"
        app:layout_constraintTop_toBottomOf="@+id/button_THREE2" />

    <Button
        android:id="@+id/button_SEVEN2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:text="7"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/button_EIGHT2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_FOUR2" />

    <Button
        android:id="@+id/button_EIGHT2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:text="8"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/button_NINE2"
        app:layout_constraintStart_toEndOf="@+id/button_SEVEN2"
        app:layout_constraintTop_toBottomOf="@+id/button_FIVE2" />

    <Button
        android:id="@+id/button_NINE2"
        android:layout_width="109dp"
        android:layout_height="74dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:text="9"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_EIGHT2"
        app:layout_constraintTop_toBottomOf="@+id/button_SIX2" />

    <Button
        android:id="@+id/button_B2D"
        android:layout_width="163dp"
        android:layout_height="74dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:text="B2D"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/button_D2B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_SEVEN2" />

    <Button
        android:id="@+id/button_D2B"
        android:layout_width="163dp"
        android:layout_height="74dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:text="D2B"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_B2D"
        app:layout_constraintTop_toBottomOf="@+id/button_EIGHT2" />


</android.support.constraint.ConstraintLayout>